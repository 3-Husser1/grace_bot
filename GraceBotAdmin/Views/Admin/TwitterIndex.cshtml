@model IEnumerable<GraceBot.Models.TwitterQuestion>

@{
    ViewBag.Title = "Manage Data";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Questions</h2>

<br/>

<div class="row">



</div>

<br/>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Text)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserScreenName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Timestamp)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
            {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Text)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserScreenName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Timestamp)
            </td>
          

            <td>
              @*  @Html.ActionLink("Update Answer", "Edit", new { id = item.ActivityId },new { @class = "btn btn-default" })*@ 
            </td>
        </tr>
    }

</table>